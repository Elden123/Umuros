<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_setings" tools:context="com.example.nolan.myapplication.settings">

    <Button
        android:layout_width="200dp"
        android:layout_height="91dp"
        android:text="Values"
        android:id="@+id/theValues"
        android:background="@mipmap/outline"
        android:textSize="35dp"
        android:textColor="#8cfff2eb"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="125dp" />

    <Button
        android:layout_width="200dp"
        android:layout_height="91dp"
        android:text="Colors"
        android:id="@+id/theColors"
        android:background="@mipmap/outline"
        android:textSize="35dp"
        android:textColor="#8cfff2eb"
        android:layout_marginBottom="117dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/theValues"
        android:layout_alignStart="@+id/theValues" />

</RelativeLayout>
